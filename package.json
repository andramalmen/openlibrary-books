        "validate": "npm-run-all --parallel format:check lint build"
    },
    "dependencies": {
        "autoprefixer": "^10.2.3",
        "isomorphic-unfetch": "^3.1.0",
        "next": "10.0.5",
        "postcss": "^8.2.4",
