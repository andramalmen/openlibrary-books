{
    "name": "books",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "eslint --ext .js,.ts,.tsx .",
        "format": "prettier --ignore-path .prettierignore --write --loglevel silent '**/*.{ts,tsx,js,json}'",
