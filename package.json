        "react-dom": "17.0.1",
        "tailwindcss": "^2.0.2"
    },
    "devDependencies": {
        "@types/node": "^14.14.22",
        "@types/react": "^17.0.0",
        "@typescript-eslint/eslint-plugin": "^4.14.0",
        "@typescript-eslint/parser": "^4.14.0",
        "eslint": "^7.18.0",
        "eslint-config-prettier": "^7.2.0",
        "npm-run-all": "^4.1.5",
        "postcss-flexbugs-fixes": "^5.0.2",
        "postcss-preset-env": "^6.7.0",
        "prettier": "^2.2.1",
        "typescript": "^4.1.3"
